------
static def foo(int i, int j, int k) {
    def v
    println(i + j - k)
    v = 42
    <caret>return testMethod(v, i, j)
}

private static def testMethod(int v, int i, int j) {
  if (i > 42) {
    println("preved!")
    return v + i
  } else {
    return v + j
  }
}