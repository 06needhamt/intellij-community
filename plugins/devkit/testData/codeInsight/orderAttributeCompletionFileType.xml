<idea-plugin>
  <id>com.intellij</id>

  <extensionPoints>
    <!--suppress PluginXmlValidity -->
    <extensionPoint name="myEp8"
                    beanClass="com.intellij.openapi.fileTypes.FileTypeExtensionPoint"/>
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
    <myEp8 id="id1" filetype="JAVA"/>
    <!--suppress PluginXmlValidity -->
    <myEp8 id="id2"/>
    <myEp8 id="id3" filetype="XML"/>
    <myEp8 id="id4" filetype="JAVA" order="before <caret>"/>
  </extensions>
</idea-plugin>