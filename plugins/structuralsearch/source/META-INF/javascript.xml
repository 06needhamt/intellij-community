<!DOCTYPE idea-plugin PUBLIC "Plugin/DTD" "http://plugins.intellij.net/plugin.dtd">
<idea-plugin url="http://www.jetbrains.com/idea">
  <extensions xmlns="com.intellij">
    <tokenindex.tokenizer language="JavaScript" implementationClass="com.intellij.tokenindex.JavaScriptTokenizer"/>
    <structuralsearch.profile implementation="com.intellij.structuralsearch.JSStructuralSearchProfile"/>

    <!--testing mode-->
    <structuralsearch.profile implementation="com.intellij.structuralsearch.extenders.JSStructuralSearchProfile2" order="last"/>

    <equivalenceDescriptorProvider implementation="com.intellij.structuralsearch.equivalence.js.JSEquivalenceDescriptorProvider"/>
  </extensions>
</idea-plugin>