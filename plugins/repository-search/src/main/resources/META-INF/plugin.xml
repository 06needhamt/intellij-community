<idea-plugin>
  <id>org.jetbrains.repository.search</id>
  <name>Jetbrains Repository Search</name>
  <vendor>JetBrains</vendor>
  <description>
    <![CDATA[
    Technical plugin, provides API for artifacts search
    ]]>
  </description>
  <extensionPoints>
    <extensionPoint name="providerFactory" interface="org.jetbrains.idea.reposearch.DependencySearchProvidersFactory" dynamic="true"/>
  </extensionPoints>
  <extensions defaultExtensionNs="com.intellij">
    <projectService serviceImplementation="org.jetbrains.idea.reposearch.DependencySearchService"/>
  </extensions>
</idea-plugin>