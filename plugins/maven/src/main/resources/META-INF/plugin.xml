<!DOCTYPE idea-plugin PUBLIC "Plugin/DTD" "http://plugins.intellij.net/plugin.dtd">
<idea-plugin>
  <name>Maven Integration</name>
  <id>org.jetbrains.idea.maven</id>
  <description>Provides means to import Maven pom.xml files and to run Maven goals</description>
  <version>2.0</version>
  <vendor url="http://www.jetbrains.com">JetBrains s.r.o.</vendor>
  <change-notes>
    <![CDATA[]]>
  </change-notes>
  <idea-version since-build="6800"/>

  <extensionPoints>
    <extensionPoint name="facetImporter" interface="org.jetbrains.idea.maven.web.FacetImporter"/>
  </extensionPoints>

  <depends optional="true" config-file="gwt-support.xml">com.intellij.gwt</depends>
  <depends optional="true" config-file="jee-support.xml">com.intellij.javaee</depends>

  <extensions defaultExtensionNs="com.intellij">
    <projectImportProvider implementation="org.jetbrains.idea.maven.project.open.MavenProjectImportProvider"/>
    <projectOpenProcessor implementation="org.jetbrains.idea.maven.project.open.MavenProjectOpenProcessor"/>
    <projectImportBuilder implementation="org.jetbrains.idea.maven.project.open.MavenImportBuilder"/>

    <stepsBeforeRunProvider implementation="org.jetbrains.idea.maven.events.MavenStepsBeforeRunProvider"/>
    <keymapExtension implementation="org.jetbrains.idea.maven.events.MavenKeymapExtension"/>
    <errorHandler implementation="com.intellij.diagnostic.ITNReporter"/>
    <configurationType implementation="org.jetbrains.idea.maven.runner.execution.MavenRunConfigurationType"/>

    <dom.fileDescription implementation="org.jetbrains.idea.maven.dom.MavenModelDomDescription"/>
    <!--<dom.fileDescription implementation="org.jetbrains.idea.maven.dom.MavenPluginModelDomDescription"/>-->
    <!--<lang.documentationProvider language="XML" implementationClass="org.jetbrains.idea.maven.dom.MavenPluginModelDocumentationProvider" order="last"/>-->
    <!--<inspectionToolProvider implementation="org.jetbrains.idea.maven.dom.MavenModelInspection"/>-->
    <!--<dom.extender domClass="org.jetbrains.idea.maven.dom.model.Configuration" extenderClass="org.jetbrains.idea.maven.dom.MavenPluginConfigurationDomExtender"/>-->
  </extensions>

  <application-components>
    <component>
      <implementation-class>org.jetbrains.idea.maven.core.MavenApplicationComponent</implementation-class>
    </component>
  </application-components>

  <project-components>
    <component>
      <implementation-class>org.jetbrains.idea.maven.core.MavenCore</implementation-class>
      <option name="workspace" value="true"/>
    </component>
    <component>
      <implementation-class>org.jetbrains.idea.maven.state.MavenProjectsManager</implementation-class>
    </component>
    <!--<component>-->
      <!--<implementation-class>org.jetbrains.idea.maven.repository.MavenIndicesManager</implementation-class>-->
    <!--</component>-->
    <component>
      <implementation-class>org.jetbrains.idea.maven.repository.MavenPluginsRepository</implementation-class>
    </component>
    <component>
      <implementation-class>org.jetbrains.idea.maven.navigator.MavenProjectNavigator</implementation-class>
    </component>
    <component>
      <implementation-class>org.jetbrains.idea.maven.project.MavenWorkspaceSettingsComponent</implementation-class>
      <option name="workspace" value="true"/>
    </component>
    <component>
      <interface-class>org.jetbrains.idea.maven.events.MavenEventsHandler</interface-class>
      <implementation-class>org.jetbrains.idea.maven.events.MavenEventsComponent</implementation-class>
    </component>
    <component>
      <implementation-class>org.jetbrains.idea.maven.runner.MavenRunner</implementation-class>
      <option name="workspace" value="true"/>
    </component>
  </project-components>

  <actions>
    <group id="Maven.Core">
      <action id="Maven.ShowSettings"
              class="org.jetbrains.idea.maven.core.action.ShowSettingsAction" text="Maven settings"
              description="Edit Maven integration settings for the current project"
              icon="/images/mavenSettings.png">
      </action>

      <action id="Maven.ShowDefaultSettings"
              class="org.jetbrains.idea.maven.core.action.ShowDefaultSettingsAction" text="Default Maven settings"
              description="Edit default Maven integration settings"
              icon="/general/applicationSettings.png">
      </action>

      <action id="Maven.ToggleOffline"
              class="org.jetbrains.idea.maven.core.action.ToggleOfflineAction" text="Toggle Offline mode"
              description="Toggle Offline mode for Maven"
              icon="/images/offlineMode.png">
      </action>

      <action id="Maven.ToggleSkipTests"
              class="org.jetbrains.idea.maven.runner.action.ToggleSkipTestsAction" text="Toggle 'Skip tests' mode"
              description="Toggle 'Skip tests' mode for Maven"
              icon="/nodes/exceptionClass.png">
      </action>

      <action id="Maven.Help"
              class="com.intellij.ide.actions.ContextHelpAction" text="Help"/>
    </group>

    <group id="Maven.OutputToolbar" description="Output toolbar">
      <action id="Maven.Rerun"
              class="org.jetbrains.idea.maven.runner.action.RerunMavenAction"
              text="Rerun"
              description="Reruns the latest Maven goal"
              icon="/actions/refreshUsages.png"/>
      <action id="Maven.PauseOutput"
              class="org.jetbrains.idea.maven.runner.action.PauseOutputAction"
              text="Pause/Resume Output"
              description="Pauses/resumes the output in the output window"
              icon="/actions/pause.png"/>
      <action id="Maven.Stop"
              class="org.jetbrains.idea.maven.runner.action.StopMavenAction"
              text="Stop"
              description="Stops the currently running Maven goal"
              icon="/actions/suspend.png"/>
      <action id="Maven.CloseOutput"
              class="org.jetbrains.idea.maven.runner.action.CloseOutputPanelAction"
              text="Close Output Panel"
              description="Closes the output window"
              icon="/actions/cancel.png"/>
    </group>

    <group id="Maven.PomTreeView">
      <action id="Maven.GroupByModules"
              class="org.jetbrains.idea.maven.navigator.action.GroupByModulesAction" text="Group by modules"
              description="Group by modules"
              icon="/nodes/ModuleClosed.png"/>
      <action id="Maven.GroupByDirectories"
              class="org.jetbrains.idea.maven.navigator.action.GroupByDirectoriesAction"
              text="Group by directories"
              description="Group by directories"
              icon="/nodes/folder.png"/>
      <action id="Maven.ShowIgnored"
              class="org.jetbrains.idea.maven.navigator.action.ShowIgnoredAction"
              text="Show ignored"
              description="Show ignored projects"
              icon="/actions/showHiddens.png"/>
      <action id="Maven.FilterPhases"
              class="org.jetbrains.idea.maven.navigator.action.FilterPhasesAction" text="Filter standard goals"
              description="Filter standard goals"
              icon="/images/filterPhases.png"/>
    </group>

    <action id="Maven.Run"
            class="org.jetbrains.idea.maven.runner.action.RunGoalsAction" text="_Run Goal"
            description="Execute selected goal"
            icon="/actions/execute.png">
    </action>

    <group id="Maven.State">
      <!--<action id="Maven.AddDependency"-->
              <!--class="org.jetbrains.idea.maven.state.action.AddDependencyAction" text="Add Dependency"-->
              <!--description="Add Dependency"-->
              <!--icon="/modules/library.png">-->
      <!--</action>-->
      <action id="Maven.Ignore"
              class="org.jetbrains.idea.maven.state.action.IgnoreProjectAction" text="_Ignore"
              description="Toggle ignore flag for Maven project">
      </action>
      <action id="Maven.ToggleProfile"
              class="org.jetbrains.idea.maven.state.action.ToggleProfileAction" text="Toggle profile"
              description="Activate/deactivate profile">
      </action>
    </group>

    <group id="Maven.Project">
      <action id="Maven.AddManagedFile" class="org.jetbrains.idea.maven.project.action.AddManageFileAction"
              text="Add Maven project" description="Add Maven project to managed files"
              icon="/general/add.png">
      </action>
      <action id="Maven.RemoveManagedFiles" class="org.jetbrains.idea.maven.project.action.RemoveManagedFilesAction"
              text="Remove Maven projects" description="Removes selected Maven projects from managed files"
              icon="/general/remove.png">
      </action>
      <action id="Maven.Reimport" class="org.jetbrains.idea.maven.project.action.ReimportAction"
              text="Re-import Maven projects" description="Re-import Maven projects into Idea project structure"
              icon="/actions/sync.png">
      </action>
      <action id="Maven.UpdateFolders" class="org.jetbrains.idea.maven.project.action.UpdateFoldersAction"
              text="Update Folders" description="Generate sources and update project folders"
              icon="/actions/sync.png">
      </action>
      <action id="Maven.DownloadArtifacts" class="org.jetbrains.idea.maven.project.action.DownloadArtifactsAction"
              text="Download artifacts" description="Download artifacts (sources, javadoc, plugins)"
              icon="/images/download.png">
      </action>
    </group>

    <group id="Maven.EventsGroup">
      <action id="Maven.BeforeCompile"
              class="org.jetbrains.idea.maven.events.actions.BeforeCompileAction" text="Execute before Make"
              description="Execute this goal before IntelliJ IDEA Make">
      </action>
      <action id="Maven.AfterCompile"
              class="org.jetbrains.idea.maven.events.actions.AfterCompileAction" text="Execute after Make"
              description="Execute this goal after IntelliJ IDEA Make">
      </action>
      <action id="Maven.BeforeRun"
              class="org.jetbrains.idea.maven.events.actions.BeforeRunAction" text="Execute before Run/debug..."
              description="Execute this goal before launching Run/Debug configuration">
      </action>
      <separator/>
      <action id="Maven.AssignShortcut"
              class="org.jetbrains.idea.maven.events.actions.AssignShortcutAction" text="Assign shortcut..."
              description="Assign shortcut to this goal">
      </action>
    </group>

    <group id="Maven.PomTreeToolbar" description="Pom Tree toolbar">
      <reference id="Maven.PomTreeView"/>
      <separator/>
      <reference id="Maven.Run"/>
      <separator/>
      <reference id="Maven.AddManagedFile"/>
      <reference id="Maven.RemoveManagedFiles"/>
      <separator/>
      <reference id="Maven.Reimport"/>
      <reference id="Maven.UpdateFolders"/>
      <reference id="Maven.DownloadArtifacts"/>
      <separator/>
      <reference id="Maven.ShowSettings"/>
      <reference id="Maven.ShowDefaultSettings"/>
      <reference id="Maven.ToggleOffline"/>
      <reference id="Maven.ToggleSkipTests"/>
      <separator/>
      <reference id="Maven.Help"/>
    </group>

    <group id="Maven.PomMenu" popup="true">
      <reference id="Maven.Run"/>
      <reference ref="RunContextGroup"/>
      <separator/>
      <reference id="Maven.RemoveManagedFiles"/>
      <separator/>
      <reference id="EditSource"/>
      <reference id="Maven.Ignore"/>
    </group>

    <group id="Maven.GoalMenu" popup="true">
      <reference id="Maven.Run"/>
      <reference ref="RunContextGroup"/>
      <separator/>
      <reference ref="Maven.EventsGroup"/>
    </group>

    <group id="Maven.ProfileMenu" popup="true">
      <reference id="Maven.ToggleProfile"/>
    </group>

    <group id="Maven.GenerateGroup" text="Generate">
      <action id="GenerateParent" text="Generate Parent" class="org.jetbrains.idea.maven.dom.generate.GenerateParentAction"/>
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </group>

  </actions>
</idea-plugin>