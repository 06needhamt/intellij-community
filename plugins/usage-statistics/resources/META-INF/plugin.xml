<idea-plugin>
  <id>org.jetbrains.usage.statistics</id>
  <name>IntelliJ Usage Statistics</name>
  <vendor>JetBrains</vendor>
  <description>Allows opening reports in Analytics Platform from "Statistics Event Log" tool window.</description>
  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">>
    <statisticsLogGroupActionsProvider implementation="com.intellij.usage.statistics.toolwindow.NavigateToAnalyticsPlatformActionsProvider"/>
    <backgroundPostStartupActivity implementation="com.intellij.usage.statistics.toolwindow.GroupsDescriptionsDownloadActivity"/>
    <registryKey key="usage.statistics.analytics.platform.url"
                 defaultValue="https://analytics.jetbrains.com/"
                 description="Url of the Analytics Platform"/>
  </extensions>

</idea-plugin>