<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="html/xml">
  <template description="Tag pair" name="t" toReformat="false" toShortenFQNames="true" value="&lt;$TAG_NAME$&gt;$END$&lt;/$TAG_NAME$&gt;">
    <variable alwaysStopAt="true" defaultValue="" expression="&quot;tag&quot;" name="TAG_NAME" />
    <context>
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_STRING" value="false" />
      <option name="XML" value="true" />
      <option name="HTML" value="true" />
      <option name="JSP" value="true" />
      <option name="COMPLETION" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
</templateSet>

